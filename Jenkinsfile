pipeline {
    agent any

    stages {
        stage('Code checkout') {
            steps {
                sh 'echo "Code checked out successfully!"'
            }
        }

        stage('Code Build') {
            steps {
                sh 'echo "Code build successfully!"'
            }
        }

        stage('Code Deploy') {
            steps {
                sh 'echo "Code deployed successfully!"'
            }
        }
    }
}
